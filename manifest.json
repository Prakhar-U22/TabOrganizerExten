{
    "manifest_version": 3,
    "name": "TabOrganizer",
    "version": "2.0",
    "description": "Manage your browser tabs efficiently.",
    "permissions": ["tabs", "storage"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "background": {
      "service_worker": "background.js"
    },
    "options_page": "options.html"
  }
  